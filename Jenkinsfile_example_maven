
pipeline {
	agent any
	stages{
		   stage ('Build'){
				steps {
					sh 'mvn claen package'
				}
         post {
           success {
				    	echo "Arhivating..."
              archiveArtifacts artifacts: '**/target/*.war'   
          }
         }
        } 
		   stage ('Build') {
				steps {
					echo "Arhivating..."
				}
		   }
		   stage ('Deploy') {
				steps {
					echo "Deploy step..."
				}
		   }
	}
} 
